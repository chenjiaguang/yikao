@font-face {
    font-family: 'ft-icon';
    src: local('ft-icon'),
        url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('ft-icon.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.ft-icon {
  font-family: "ft-icon" !important;
  font-size: 24rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.c-icon-search:before {
  content: "\e636";
}

.c-icon-sanjiao:before {
  content: "\e629";
}

.c-icon-xinghao:before {
  content: "\e64d";
}

.c-icon-pull_down:before {
  content: "\e61d";
}

.c-icon-tip:before {
  content: "\e704";
}

.c-icon-next:before {
  content: "\e661";
}

.c-icon-success:before {
  content: "\e605";
}